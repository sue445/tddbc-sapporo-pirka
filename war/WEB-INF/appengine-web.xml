<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>tddbc-sapporo</application>
	<version>1</version>

	<static-files>
		<include path="/**.*"/>
		<!-- GAE handles .html files as static files by default, which prevents Pirka from working. -->
		<exclude path="/**.html" />
	</static-files>

	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties" />
	</system-properties>
</appengine-web-app>
